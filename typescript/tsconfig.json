{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "ES2017",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "build",
      "baseUrl": ".",
      "paths": {
          "models/*": [
              "src/models/*",
          ],
          "types/*": [
              "src/types/*",
          ],
          "utils/*": [
              "src/utils/*",
          ],
          "services/*": [
              "src/services/*"
          ],
          "*": [
              "node_modules/*",
          ]
      },
  },
  "include": [
      "src/**/*"
  ],
}
